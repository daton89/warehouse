    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.1.7",
    "pm2": "^3.5.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "~2.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.8",
